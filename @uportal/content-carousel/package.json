{
  "name": "@uportal/content-carousel",
  "version": "1.0.0",
  "scripts": {
    "build": "vue-cli-service build --name content-carousel --target wc",
    "test": "vue-cli-service lint",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-carousel": "^0.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "@vue/eslint-config-prettier": "^3.0.0-beta.11",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "root": true,
    "env": {
      "node": true
    },
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "publishConfig": {
    "access": "public"
  },
  "description": "Display portlet content in a browsable carousel",
  "bugs": {
    "url": "https://github.com/uPortal-contrib/uPortal-web-components/issues"
  },
  "license": "Apache-2.0",
  "author": "Apereo <uportal-dev@apereo.org>",
  "keywords": [
    "apereo",
    "uportal",
    "content",
    "carousel",
    "browse",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uPortal-contrib/uPortal-web-components.git"
  },
  "homepage": "https://uportal-contrib.github.io/uPortal-web-components/"
}
