  "main": "dist/eyebrow-user-info.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uPortal-contrib/uPortal-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/uPortal-contrib/uPortal-web-components/issues"
