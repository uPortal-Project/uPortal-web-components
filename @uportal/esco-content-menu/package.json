    "build": "vue-cli-service build --name esco --target wc 'src/components/*.vue'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@uportal/open-id-connect": "^1.20.0",
    "lodash": "^4.17.11",
    "vue": "^2.5.20",
    "vue-awesome-swiper": "^3.1.3",
    "vue-i18n": "^8.4.0",
    "vue-simple-spinner": "^1.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/free-regular-svg-icons": "^5.6.0",
    "@fortawesome/free-solid-svg-icons": "^5.6.0",
    "@fortawesome/vue-fontawesome": "^0.1.3",
    "@vue/cli-plugin-babel": "^3.2.2",
    "@vue/cli-plugin-eslint": "^3.2.2",
    "@vue/cli-service": "^3.2.3",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
